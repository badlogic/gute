{
	"name": "gute",
	"version": "1.0.22",
	"description": "Simple Web Game Library",
	"main": "dist/bundle.js",
	"types": "dist/bundle.d.ts",
	"scripts": {
		"doc": "mv docs/test .; typedoc src/index.ts --out docs; mv test docs",
		"prepare": "npm run doc; webpack; webpack --config webpack.config.prod.js",
		"version": "git add -A src",
		"postversion": "git push && git push --tags",
		"build": "webpack; webpack --config webpack.config.prod.js",
		"watch": "webpack --watch"
	},
	"files": [
		"dist/**/*"
	],
	"author": "Kevin Glass",
	"license": "MIT",
	"devDependencies": {
		"ts-loader": "^9.1.2",
		"typedoc": "^0.20.36",
		"typescript": "^4.2.4",
		"webpack": "^5.37.0",
		"webpack-cli": "^4.7.0"
	}
}
